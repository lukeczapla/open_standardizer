<?xml version="1.0" encoding="UTF-8"?>
<StandardizerConfiguration Version="0.1">
  <Actions>
    <RemoveFragment Method="keepLargest"/>

    <!-- nostereo = DO NOT clear stereochemistry -->

    <RemoveAttachedData/>
    <ClearAtomValues/>
    <RemoveExplicitH/>

    <!-- Explicit isotope clearing -->
    <ClearIsotopes/>

    <Neutralize/>
    <Aromatize Type="general"/>
    <Mesomerize/>
    <Tautomerize/>

    <CleanGeometry dimension="2D"/>
  </Actions>
</StandardizerConfiguration>
